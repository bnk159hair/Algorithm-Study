   "source": [
    "n = int(input())\n",
    "\n",
    "arr = list(map(int, input().split()))\n",
    "\n",
    "for i in range(n-1):\n",
    "    min_val = i\n",
    "    for j in range(i+1, n):\n",
    "        if arr[j]<arr[min_val]:\n",
    "            min_val = j\n",
    "    tmp = arr[min_val]\n",
    "    arr[min_val]= arr[i]\n",
    "    arr[i]=tmp\n",
    "\n",
    "print(\" \".join(str(i) for i in arr))"
   ]
