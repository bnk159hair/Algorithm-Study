   "source": [
    "n = int(input())\n",
    "arr = list(map(int, input().split()))\n",
    "\n",
    "count = 0\n",
    "\n",
    "for i in range(n):\n",
    "    for j in range(i+1, n):\n",
    "        for k in range(j+1, n):\n",
    "            if (arr[i] <= arr[j]) and (arr[j] <= arr[k]):\n",
    "                count += 1\n",
    "\n",
    "print(count) \n"
   ]
  }
