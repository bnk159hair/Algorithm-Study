   "source": [
    "# 변수 선언 및 입력\n",
    "n, m = tuple(map(int, input().split()))\n",
    "arr1 = list(map(int, input().split()))\n",
    "arr2 = list(map(int, input().split()))\n",
    "\n",
    "# 모든 구간의 시작점을 잡아봅니다.\n",
    "cnt = 0\n",
    "for i in range(0, n - m + 1):\n",
    "\tif sorted(arr1[i:i+m]) == sorted(arr2):\n",
    "\t\tcnt += 1\n",
    "print(cnt)\n"
   ]
  }
