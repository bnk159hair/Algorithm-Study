   "source": [
    "string_1 = input()\n",
    "string_2 = input()\n",
    "\n",
    "def findstring():\n",
    "    for i in range(len(string_1)-len(string_2)+1):\n",
    "        if string_1[i:i+len(string_2)] == string_2:\n",
    "            return i\n",
    "    return -1\n",
    "\n",
    "print(findstring())"
   ]
