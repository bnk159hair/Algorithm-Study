   "source": [
    "n = int(input())\n",
    "arr = [list(map(int, input().split())) \n",
    "for _ in range(n)]\n",
    "\n",
    "max_num = 0\n",
    "for i in range(n):\n",
    "    for j in range(n-2):\n",
    "        coin = arr[i][j]+arr[i][j+1]+arr[i][j+2]\n",
    "        max_num = max(max_num, coin)\n",
    "\n",
    "print(max_num)\n",
    "\n"
   ]
  }
