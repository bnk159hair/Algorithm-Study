   "source": [
    "from collections import deque \n",
    "N = int(input())\n",
    "dq = deque()\n",
    "for i in range(1, N+1):\n",
    "    dq.append(i)\n",
    "\n",
    "while len(dq) != 1:\n",
    "    dq.popleft()\n",
    "    if len(dq) == 1:\n",
    "        break\n",
    "    dq.append(dq[0])\n",
    "    dq.popleft()\n"
   ]
  }
